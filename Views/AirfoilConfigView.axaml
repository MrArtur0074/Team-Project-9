<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:Project_9.ViewModels"
             x:Class="Project_9.Views.AirfoilConfigView"
             x:DataType="vm:AirfoilConfigViewModel">

    <StackPanel Margin="10">
        <Button Content="Import Root Airfoil" Command="{Binding ImportRootAirfoilCommand}" Margin="5" />
        <Button Content="Import Tip Airfoil" Command="{Binding ImportTipAirfoilCommand}" Margin="5" />

        <TextBlock Text="Root Airfoil:" Margin="5" />
        <TextBlock Text="{Binding RootAirfoil.Name}" Margin="5" />

        <TextBlock Text="Tip Airfoil:" Margin="5" />
        <TextBlock Text="{Binding TipAirfoil.Name}" Margin="5" />
    </StackPanel>
</UserControl>